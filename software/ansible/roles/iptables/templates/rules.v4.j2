## Generated by Ansible (iptables)
*filter
:INPUT DROP [0:0]
:FORWARD DROP [0:0]
:OUTPUT ACCEPT [0:0]
-A INPUT -i lo -j ACCEPT
-A INPUT -m state --state RELATED,ESTABLISHED -j ACCEPT
-A INPUT -m state --state INVALID -j DROP
{% for type in IPTABLES_ICMPV4_TYPES|flatten|unique -%}
-A INPUT -p icmp -m icmp --icmp-type {{ type }} -j ACCEPT
{% endfor -%}
{% for port in IPTABLES_TCP_PORTS|flatten|unique -%}
-A INPUT -p tcp -m tcp --dport {{ port }} -j ACCEPT
{% endfor -%}
{% for port in IPTABLES_UDP_PORTS|flatten|unique -%}
-A INPUT -p udp -m udp --dport {{ port }} -j ACCEPT
{% endfor -%}
COMMIT
