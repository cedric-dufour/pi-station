## Generated by Ansible (ogn)

RF:
{
  # Path ot the RF <-> Decoder FIFO file
  PipeName = "/run/ogn-rf.fifo";

  # [ppm] "big" R820T sticks have 40-80ppm correction factors, measure it with gsm_scan
  FreqCorr = {{ OGN_CONF_RF_FREQCORR }};

  # for OGN (FLARM) reception
  OGN:
  {
    # [int] 0=Auto, 1=Manual, 2=Linearity, 3=Sensitivity
    GainMode = {{ OGN_CONF_RF_OGN_GAIN_MODE }};
    # [dB]  RF input gain (high-gain antennas may require lower input gain)
    Gain     = {{ OGN_CONF_RF_OGN_GAIN }};
  } ;

  # for frequency calibration based on GSM signals
  GSM:
  {
    # [MHz] find the best GSM frequency with gsm_scan
    CenterFreq = {{ OGN_CONF_RF_GSM_CENTERFREQ }};
    # [dB]  RF input gain (beware that GSM signals are very strong !)
    Gain       = {{ OGN_CONF_RF_GSM_GAIN }};
  } ;
} ;

Position:
{
  # [deg] Antenna coordinates
  Latitude   = {{ OGN_CONF_POSITION_LATITUDE }};
  Longitude  = {{ OGN_CONF_POSITION_LONGITUDE }};
  # [m]   Altitude above sea leavel
  Altitude   = {{ OGN_CONF_POSITION_ALTITUDE }};
  # [m]   Geoid separation: FLARM transmits GPS altitude, APRS uses means Sea level altitude
  GeoidSepar = {{ OGN_CONF_POSITION_GEOIDSEPAR }};
} ;

APRS:
{
  # APRS callsign (max. 9 characters); please refer to http://wiki.glidernet.org/receiver-naming-convention
  Call = "{{ OGN_CONF_APRS_CALL }}";
} ;

HTTP:
{
  # [int] Port of the internal HTTP server (0 = disable)
  Port = {{ OGN_CONF_HTTP_PORT }};
} ;
